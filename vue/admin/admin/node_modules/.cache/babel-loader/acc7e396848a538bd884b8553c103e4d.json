caipinleixingChange\" clearable v-model=\"searchForm.caipinleixing\" placeholder=\"请选择菜品类型\">\n\t\t\t\t\t\t\t<el-option v-for=\"(item,index) in caipinleixingOptions\" v-bind:key=\"index\" :label=\"item\" :value=\"item\"></el-option>\n\t\t\t\t\t\t</el-select>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div :style='{\"margin\":\"0 10px 0 0\",\"display\":\"inline-block\"}'>\r\n\t\t\t\t\t\t<label :style='{\"margin\":\"0 10px 0 0\",\"color\":\"#666\",\"display\":\"inline-block\",\"lineHeight\":\"50px\",\"fontSize\":\"16px\",\"fontWeight\":\"500\",\"height\":\"50px\"}' class=\"item-label\">口味</label>\n\t\t\t\t\t\t<el-input v-model=\"searchForm.kouwei\" placeholder=\"口味\" clearable></el-input>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div :style='{\"margin\":\"0 10px 0 0\",\"display\":\"inline-block\"}'>\r\n\t\t\t\t\t\t<label :style='{\"margin\":\"0 10px 0 0\",\"color\":\"#666\",\"display\":\"inline-block\",\"lineHeight\":\"50px\",\"fontSize\":\"16px\",\"fontWeight\":\"500\",\"height\":\"50px\"}' class=\"item-label\">价格</label>\n\t\t\t\t\t\t<el-input v-model=\"searchForm.pricestart\" placeholder=\"最小价格\" clearable></el-input>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div :style='{\"margin\":\"0 10px 0 0\",\"display\":\"inline-block\"}' :label=\"'至'\">\n\t\t\t\t\t\t<el-input v-model=\"searchForm.priceend\" placeholder=\"最大价格\" clearable></el-input>\n\t\t\t\t\t</div>\n\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #badcdc\",\"cursor\":\"pointer\",\"padding\":\"0 34px\",\"boxShadow\":\"inset 0px -3px 2px #badcdc\",\"margin\":\"0 0px 0 10px\",\"color\":\"#333\",\"minWidth\":\"100px\",\"outline\":\"none\",\"borderRadius\":\"20px\",\"background\":\"#deeeee\",\"width\":\"auto\",\"fontSize\":\"16px\",\"height\":\"44px\"}' type=\"success\" @click=\"search()\">查询</el-button>\n\t\t\t\t</el-row>\n\n\t\t\t\t<el-row :style='{\"padding\":\"16px 12px 10px\",\"margin\":\"0 0 0px\",\"borderColor\":\"#e2f0e2\",\"flexWrap\":\"wrap\",\"background\":\"#fff\",\"borderWidth\":\"0px\",\"display\":\"flex\",\"width\":\"auto\",\"borderStyle\":\"solid\",\"justifyContent\":\"center\",\"zIndex\":\"999\"}'>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 20px\",\"boxShadow\":\"inset 0px -3px 2px #ececde\",\"margin\":\"0 10px 6px 0\",\"borderColor\":\"#ececde\",\"color\":\"#333\",\"minWidth\":\"90px\",\"outline\":\"none\",\"borderRadius\":\"20px\",\"background\":\"#fff\",\"borderWidth\":\"1px\",\"width\":\"auto\",\"fontSize\":\"16px\",\"borderStyle\":\"solid\",\"height\":\"44px\"}' v-if=\"isAuth('caipinxinxi','新增')\" type=\"success\" @click=\"addOrUpdateHandler()\">新增</el-button>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"boxShadow\":\"inset 0px -3px 2px #ececde\",\"margin\":\"0 10px 6px 0\",\"borderColor\":\"#ececde\",\"color\":\"#333\",\"minWidth\":\"90px\",\"outline\":\"none\",\"borderRadius\":\"20px\",\"background\":\"#fff\",\"borderWidth\":\"1px\",\"width\":\"auto\",\"fontSize\":\"16px\",\"borderStyle\":\"solid\",\"height\":\"44px\"}' v-if=\"isAuth('caipinxinxi','删除')\" :disabled=\"dataListSelections.length <= 0\" type=\"danger\" @click=\"deleteHandler()\">删除</el-button>\n\n\n\n\n\t\t\t\t</el-row>\n\t\t\t</el-form>\r\n\t\t\t\n\t\t\t<!-- <div> -->\n\t\t\t\t<el-table class=\"tables\"\r\n\t\t\t\t\t:stripe='true'\r\n\t\t\t\t\t:style='{\"padding\":\"0\",\"borderColor\":\"#e2f0e2\",\"margin\":\"0 0 20px\",\"borderRadius\":\"5px\",\"borderWidth\":\"1px 0 0 1px\",\"background\":\"#fff\",\"width\":\"calc(100% - 0px)\",\"borderStyle\":\"solid\"}' \r\n\t\t\t\t\tv-if=\"isAuth('caipinxinxi','查看')\"\n\t\t\t\t\t:data=\"dataList\"\n\t\t\t\t\tv-loading=\"dataListLoading\"\n\t\t\t\t@selection-change=\"selectionChangeHandler\">\n\t\t\t\t\t<el-table-column :resizable='true' type=\"selection\" align=\"center\" width=\"50\"></el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true' label=\"索引\" type=\"index\" width=\"50\" />\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"caipinmingcheng\"\n\t\t\t\t\tlabel=\"菜品名称\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.caipinmingcheng}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"caipinleixing\"\n\t\t\t\t\tlabel=\"菜品类型\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.caipinleixing}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true' prop=\"tupian\" width=\"200\" label=\"图片\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<div v-if=\"scope.row.tupian\">\n\t\t\t\t\t\t\t\t<img v-if=\"scope.row.tupian.substring(0,4)=='http'\" :src=\"scope.row.tupian.split(',')[0]\" width=\"100\" height=\"100\">\n\t\t\t\t\t\t\t\t<img v-else :src=\"$base.url+scope.row.tupian.split(',')[0]\" width=\"100\" height=\"100\">\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div v-else>无图片</div>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"kouwei\"\n\t\t\t\t\tlabel=\"口味\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.kouwei}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"yujishijian\"\n\t\t\t\t\tlabel=\"预计时间\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.yujishijian}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"fabushijian\"\n\t\t\t\t\tlabel=\"发布时间\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.fabushijian}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='true'  \n\t\t\t\t\t\tprop=\"price\"\n\t\t\t\t\tlabel=\"价格\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.price}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column width=\"300\" label=\"操作\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #85bfeb\",\"cursor\":\"pointer\",\"padding\":\"0 10px 0 26px\",\"margin\":\"0 5px 6px\",\"outline\":\"none\",\"color\":\"#1e98f5\",\"borderRadius\":\"20px\",\"background\":\"url(http://codegen.caihongy.cn/20230329/38553f099fa74a2d86e2206c373a9539.png) no-repeat 5px center / 20px\",\"width\":\"auto\",\"fontSize\":\"15px\",\"height\":\"32px\"}' v-if=\" isAuth('caipinxinxi','查看')\" type=\"success\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id,'info')\">详情</el-button>\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #a4dbdd\",\"cursor\":\"pointer\",\"padding\":\"0 10px 0 26px\",\"margin\":\"0 5px 6px\",\"outline\":\"none\",\"color\":\"#15a4aa\",\"borderRadius\":\"20px\",\"background\":\"url(http://codegen.caihongy.cn/20230329/99f867042acb4c4aae637c8bfcf6714a.png) no-repeat 3px center\",\"width\":\"auto\",\"fontSize\":\"15px\",\"height\":\"32px\"}' v-if=\" isAuth('caipinxinxi','修改')\" type=\"primary\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button>\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #85bfeb\",\"cursor\":\"pointer\",\"padding\":\"0 10px 0 26px\",\"margin\":\"0 5px 6px\",\"outline\":\"none\",\"color\":\"#1e98f5\",\"borderRadius\":\"20px\",\"background\":\"url(http://codegen.caihongy.cn/20230329/38553f099fa74a2d86e2206c373a9539.png) no-repeat 5px center / 20px\",\"width\":\"auto\",\"fontSize\":\"15px\",\"height\":\"32px\"}' v-if=\"isAuth('caipinxinxi','查看评论')\" type=\"primary\" size=\"mini\" @click=\"disscussListHandler(scope.row.id)\">查看评论</el-button>\n\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #e68080\",\"cursor\":\"pointer\",\"padding\":\"0 10px 0 26px\",\"margin\":\"0 5px 6px\",\"outline\":\"none\",\"color\":\"#c00\",\"borderRadius\":\"20px\",\"background\":\"url(http://codegen.caihongy.cn/20230329/c5ea125f385740a3a3b9aed07f5dfe2c.png) no-repeat 5px center / 20px\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('caipinxinxi','删除') \" type=\"danger\" size=\"mini\" @click=\"deleteHandler(scope.row.id)\">删除</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t</el-table>\n\t\t\t\t<el-pagination\r\n\t\t\t\t\t@size-change=\"sizeChangeHandle\"\n\t\t\t\t\t@current-change=\"currentChangeHandle\"\n\t\t\t\t\t:current-page=\"pageIndex\"\n\t\t\t\t\tbackground\r\n\t\t\t\t\t:page-sizes=\"[10, 20, 30, 50]\"\r\n\t\t\t\t\t:page-size=\"pageSize\"\r\n\t\t\t\t\t:layout=\"layouts.join()\"\r\n\t\t\t\t\t:total=\"totalPage\"\r\n\t\t\t\t\tprev-text=\"<\"\r\n\t\t\t\t\tnext-text=\">\"\r\n\t\t\t\t\t:hide-on-single-page=\"true\"\r\n\t\t\t\t\t:style='{\"width\":\"calc(100% - 370px)\",\"padding\":\"0\",\"margin\":\"0 170px 0px 200px\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"fontWeight\":\"500\"}'\n\t\t\t\t></el-pagination>\n\t\t\t<!-- </div> -->\n\t\t</template>\r\n\t\t\n\t\t<!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n\t\t<add-or-update v-if=\"addOrUpdateFlag\" :parent=\"this\" ref=\"addOrUpdate\"></add-or-update>\n\n\n\n\n\n\t</div>\n</template>\r\n\n<script>\nimport axios from 'axios'\nimport AddOrUpdate from \"./add-or-update\";\nexport default {\n  data() {\n    return {\r\n      caipinleixingOptions: [],\n      searchForm: {\n        key: \"\"\n      },\n      form:{},\n      dataList: [],\n      pageIndex: 1,\n      pageSize: 10,\n      totalPage: 0,\n      dataListLoading: false,\n      dataListSelections: [],\n      showFlag: true,\n      sfshVisiable: false,\n      shForm: {},\n      chartVisiable: false,\n      chartVisiable1: false,\n      chartVisiable2: false,\n      chartVisiable3: false,\n      chartVisiable4: false,\n      chartVisiable5: false,\n      addOrUpdateFlag:false,\n      layouts: [\"total\",\"prev\",\"pager\",\"next\",\"sizes\",\"jumper\"],\n\n    };\n  },\n  crea